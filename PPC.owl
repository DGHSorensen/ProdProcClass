    <!-- http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#Arrange -->

    <owl:Class rdf:about="http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#Arrange">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#Movement"/>
        <dc:description>Arranging is the movement of bodies from an undefined to a predetermined orientation and position. It serves to create a defined spatial arrangement of several similar bodies. It is defined by specifying the actual and the desired arrangement of the body.</dc:description>
        <dc:title>Arrange</dc:title>
    </owl:Class>
    


