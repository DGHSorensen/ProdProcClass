        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#Handling"/>
        <dc:description>The creation of a new quantity of geometrically determined bodies from an existing quantity. An existing quantity (or quantities) are either divided or combined into a new quantity.</dc:description>
        <dc:title>Change Quantities
Alternateive: Varitation of Quantities</dc:title>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#CheckColour -->

    <owl:Class rdf:about="http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#CheckColour">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#CheckProperties"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Check colour is the determination of whether bodies have a predetermined reference colour.</dc:description>
        <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Check colour</dc:title>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#CheckIdentity -->

    <owl:Class rdf:about="http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#CheckIdentity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#CheckProperties"/>
        <dc:description>Check identity is the determination of whether a body meets certain identifying properties. Identity may be checked both through test of physical properties of the body (such as weight, shape and size etc.) or via other identification methods such as barcodes, RFID tags or NFC tags.</dc:description>
        <dc:title>Check Identity</dc:title>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#CheckOrientation -->

