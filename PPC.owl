    <!-- http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#Annealing -->

    <owl:Class rdf:about="http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#Annealing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/daniel/ontologies/2017/9/PPC#ChangeMaterialProperties"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Annealing is the heat treatment of a material to improve certain mechanical and/or chemical properties. Material is heated to a specific temperature range and held there for a certain amount of time. The material is then cooled at a suitable rate and the desired properties are achieved.</dc:description>
        <dc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Annealing</dc:title>
    </owl:Class>
    


